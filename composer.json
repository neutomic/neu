{
    "name": "neutomic/neu",
    "type": "library",
    "description": "Neu - a modern, concurrent, fast framework for web development",
    "license": "MIT",
    "authors": [
        {
            "name": "azjezz",
            "email": "azjezz@protonmail.com"
        },
        {
            "name": "contributors",
            "homepage": "https://github.com/neutomic/neu/graphs/contributors"
        }
    ],
    "require": {
        "php": "~8.1.0",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-sodium": "*",
        "amphp/amp": "v3.x-dev",
        "azjezz/psl": "2.0.x-dev",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "amphp/cache": "v2.x-dev",
        "friendsofphp/php-cs-fixer": "^3.4.0",
        "php-coveralls/php-coveralls": "^2.5",
        "phpunit/phpunit": "^9.5.10",
        "psr/cache": "^1.0 || ^2.0 || ^3.0",
        "psr/simple-cache": "^1.0 || ^2.0 || ^3.0",
        "squizlabs/php_codesniffer": "^3.6.2",
        "symfony/cache-contracts": "^2.5 || ^3.0",
        "symfony/cache": "^6.0",
        "cache/array-adapter": "^1.1",
        "psalm/phar": "^4.17",
        "php-standard-library/psalm-plugin": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Neu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neu\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
