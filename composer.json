{
    "name": "neutomic/neu",
    "type": "library",
    "description": "Neu - a modern, concurrent, fast framework for web development",
    "license": "MIT",
    "authors": [
        {
            "name": "azjezz",
            "email": "azjezz@protonmail.com"
        },
        {
            "name": "contributors",
            "homepage": "https://github.com/neutomic/neu/graphs/contributors"
        }
    ],
    "require": {
        "php": "~8.1.0",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-sodium": "*",
        "amphp/amp": "v3.x-dev",
        "azjezz/psl": "2.0.x-dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4.0",
        "php-coveralls/php-coveralls": "^2.5",
        "php-standard-library/psalm-plugin": "^1.1.2",
        "phpunit/phpunit": "^9.5.10",
        "psalm/phar": "^4.11.2",
        "squizlabs/php_codesniffer": "^3.6.1"
    },
    "autoload": {
        "psr-4": {
            "Neu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neu\\Tests\\": "tests/"
        }
    }
}
