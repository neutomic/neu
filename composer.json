{
    "name": "neutomic/neu",
    "type": "library",
    "description": "Neu - a modern, concurrent, fast framework for web development",
    "license": "MIT",
    "authors": [
        {
            "name": "azjezz",
            "email": "azjezz@protonmail.com"
        },
        {
            "name": "trowski",
            "email": "aaron@trowski.com"
        },
        {
            "name": "contributors",
            "homepage": "https://github.com/neutomic/neu/graphs/contributors"
        }
    ],
    "require": {
        "php": "~8.2.0",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-sodium": "*",
        "amphp/amp": "^3.0.0",
        "azjezz/psl": "^2.5.0",
        "psr/event-dispatcher": "^1.0",
        "amphp/postgres": "2.x-dev"
    },
    "require-dev": {
        "amphp/cache": "v2.x-dev",
        "friendsofphp/php-cs-fixer": "^3.15.1",
        "php-coveralls/php-coveralls": "^2.5.3",
        "phpunit/phpunit": "^9.6.5",
        "psr/cache": "^1.0 || ^2.0 || ^3.0",
        "psr/simple-cache": "^1.0 || ^2.0 || ^3.0",
        "squizlabs/php_codesniffer": "^3.7.2",
        "symfony/cache-contracts": "^2.5.2 || ^3.0",
        "symfony/cache": "^6.2.7",
        "cache/array-adapter": "^1.2",
        "psalm/phar": "^5.8.0"
    },
    "autoload": {
        "psr-4": {
            "Neu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neu\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
